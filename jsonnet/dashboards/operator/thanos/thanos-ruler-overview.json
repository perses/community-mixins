                    "query": "sum by (namespace, job) (\n  rate(thanos_alert_queue_alerts_pushed_total{job=~\"$job\",namespace=\"$namespace\"}[$__rate_interval])\n)",
                    "seriesNameFormat": "{{job}} {{namespace}}"
                  }
                }
              }
