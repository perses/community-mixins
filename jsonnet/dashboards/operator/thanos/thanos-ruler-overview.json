{
  "kind": "PersesDashboard",
  "apiVersion": "perses.dev/v1alpha1",
  "metadata": {
    "name": "thanos-ruler-overview",
    "namespace": "perses-dev",
    "creationTimestamp": null,
    "labels": {
      "app.kubernetes.io/component": "dashboard",
      "app.kubernetes.io/instance": "thanos-ruler-overview",
      "app.kubernetes.io/name": "perses-dashboard",
      "app.kubernetes.io/part-of": "perses-operator"
    }
  },
  "spec": {
    "display": {
      "name": "Thanos / Ruler / Overview"
    },
    "variables": [
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "job",
            "hidden": false
          },
          "allowAllValue": false,
          "allowMultiple": true,
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "datasource": {
                "kind": "PrometheusDatasource",
                "name": "prometheus-datasource"
              },
              "labelName": "job",
              "matchers": [
                "thanos_build_info{container=\"thanos-rule\"}"
              ]
            }
          },
          "name": "job"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "namespace",
            "hidden": false
          },
          "allowAllValue": false,
          "allowMultiple": false,
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "datasource": {
                "kind": "PrometheusDatasource",
                "name": "prometheus-datasource"
              },
              "labelName": "namespace",
              "matchers": [
                "thanos_status{}"
              ]
            }
          },
          "name": "namespace"
        }
      }
    ],
    "panels": {
      "0_0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Rule Group Evaluation Rate",
            "description": "Shows the rate of rule group evaluations."
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom",
                "mode": "table"
              },
              "yAxis": {
                "format": {
                  "unit": "counts/sec"
                }
              },
              "visual": {
                "display": "line",
                "lineWidth": 0.25,
                "areaOpacity": 0.5,
                "palette": {
                  "mode": "auto"
                }
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "sum by (namespace, job, rule_group, strategy) (\n  rate(prometheus_rule_evaluations_total{job=~\"$job\",namespace=\"$namespace\"}[$__rate_interval])\n)",
                    "seriesNameFormat": "{{rule_group}} {{strategy}} - {{job}} {{namespace}}"
                  }
                }
              }
            }
          ]
        }
      },
      "0_1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Rule Group Evaluation Failures",
            "description": "Shows the rate of failed rule group evaluations."
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom",
                "mode": "table"
              },
              "yAxis": {
                "format": {
                  "unit": "counts/sec"
                }
              },
              "visual": {
                "display": "line",
                "lineWidth": 0.25,
                "areaOpacity": 1,
                "palette": {
                  "mode": "auto"
                },
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "sum by (namespace, job, rule_group, strategy) (\n  rate(\n    prometheus_rule_evaluation_failures_total{job=~\"$job\",namespace=\"$namespace\"}[$__rate_interval]\n  )\n)",
                    "seriesNameFormat": "{{rule_group}} {{strategy}} - {{job}} {{namespace}}"
                  }
                }
              }
            }
          ]
        }
      },
      "0_2": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Rule Group Evaluations Missed",
            "description": "Shows rate of rule group evaluations missed due to slow rule evaluations."
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom",
                "mode": "table"
              },
              "yAxis": {
                "format": {
                  "unit": "counts/sec"
                }
              },
              "visual": {
                "display": "line",
                "lineWidth": 0.25,
                "areaOpacity": 1,
                "palette": {
                  "mode": "auto"
                },
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "sum by (namespace, job, rule_group, strategy) (\n  rate(\n    prometheus_rule_group_iterations_missed_total{job=~\"$job\",namespace=\"$namespace\"}[$__rate_interval]\n  )\n)",
                    "seriesNameFormat": "{{rule_group}} {{strategy}} - {{job}} {{namespace}}"
                  }
                }
              }
            }
          ]
        }
      },
      "0_3": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Rule Group Evaluations Too Slow",
            "description": "Shows rule groups with evaluations taking longer than their set interval."
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom",
                "mode": "table"
              },
              "yAxis": {
                "format": {
                  "unit": "seconds"
                }
              },
              "visual": {
                "display": "line",
                "lineWidth": 0.25,
                "areaOpacity": 0.5,
                "palette": {
                  "mode": "auto"
                }
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "  sum by (namespace, job, rule_group) (\n    prometheus_rule_group_last_duration_seconds{job=~\"$job\",namespace=\"$namespace\"}\n  )\n\u003e\n  sum by (namespace, job, rule_group) (\n    prometheus_rule_group_interval_seconds{job=~\"$job\",namespace=\"$namespace\"}\n  )",
                    "seriesNameFormat": "{{rule_group}} - {{job}} {{namespace}}"
                  }
                }
              }
            }
          ]
        }
      },
      "1_0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Alerts Sent",
            "description": "Shows rate of alerts sent by ruler."
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom",
                "mode": "table"
              },
              "yAxis": {
                "format": {
                  "unit": "counts/sec"
                }
              },
              "visual": {
                "display": "line",
                "lineWidth": 0.25,
                "areaOpacity": 1,
                "palette": {
                  "mode": "auto"
                },
                "stack": "all"
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource",
                      "name": "prometheus-datasource"
                    },
                    "query": "sum by (namespace, job, alertmanager) (\n  rate(thanos_alert_sender_alerts_sent_total{job=~\"$job\",namespace=\"$namespace\"}[$__rate_interval])\n)",
                    "seriesNameFormat": "{{alertmanager}} - {{job}} {{namespace}}"
                  }
                }
              }
            }
          ]
        }
